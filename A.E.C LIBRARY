executorBox.PlaceholderText = "Enter script here..."


local executeButton = Instance.new("TextButton", executorFrame)

executeButton.Size = UDim2.new(0.4, 0, 0.1, 0)

executeButton.Position = UDim2.new(0.05, 0, 0.85, 0)

executeButton.Text = "Execute"

executeButton.TextColor3 = Color3.new(1, 0, 0)

executeButton.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)

executeButton.Font = Enum.Font.SciFi

executeButton.TextSize = 14

executeButton.MouseButton1Click:Connect(function()

    loadstring(executorBox.Text)()

end)
