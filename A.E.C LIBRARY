executorBox.Position = UDim2.new(0.05, 0, 0.1, 0)

executorBox.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)

executorBox.TextColor3 = Color3.new(1, 0, 0)

executorBox.Font = Enum.Font.SciFi

